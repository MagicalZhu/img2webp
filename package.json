{
  "publisher": "Athu",
  "name": "img2webp",
  "displayName": "img2webp",
  "version": "1.0.1",
  "private": false,
  "packageManager": "pnpm@8.1.1",
  "description": "",
  "author": "Athu <huakucha95@163.com>",
  "license": "MIT",
  "homepage": "https://github.com/MagicalZhu/img2webp#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/MagicalZhu/img2webp"
  },
  "bugs": {
    "url": "https://github.com/MagicalZhu/img2webp/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.69.0"
  },
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "convert",
        "title": "Convert-To-Webp"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "convert",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@antfu/ni": "^0.21.2",
    "@types/node": "^18.15.11",
    "@types/vscode": "^1.69.0",
    "bumpp": "^9.1.0",
    "eslint": "^8.37.0",
    "esno": "^0.16.3",
    "pnpm": "^8.1.1",
    "rimraf": "^4.4.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vitest": "^0.29.8",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "@squoosh/lib": "^0.5.3",
    "@types/recursive-readdir": "^2.2.1",
    "recursive-readdir": "^2.2.3"
  }
}
